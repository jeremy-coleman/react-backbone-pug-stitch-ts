{
  "compilerOptions": {
    "outDir": "static",
    "allowJs": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "lib": ["dom","dom.iterable", "es6", "es7", "es2017", "esnext"],
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noImplicitThis": false,
    "noUnusedLocals": false,
    "strict": false,
    "pretty": true
  },
  "include":["src"],
  "exclude": ["node_modules"]
}
